<h1>Projects</h1>

<div class="card-header d-flex justify-content-between">
  <span>Projects</span>
  <%= link_to 'New Project', new_project_path, class: 'btn btn-primary' %>
</div>

<ul class="list-group">
  <% @projects.each do |project| %>
    <li class="list-group-item">
      <%= link_to project.name, project %>
    </li>
  <% end %>
</ul>



